{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npm run lint && vite --host",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run eslint && npm run lint:css",
    "lint:css": "stylelint *.{less,css} --fix  --custom-syntax postcss-less",
    "pretty": "prettier --write .",
    "eslint": "eslint --fix ./src",
    "generate-css-typesInit": "tailwindcss-classnames -i tailwind.config.js -o tailwindcss-classnames.ts",
    "generate-css-typesExpand": "tailwindcss-classnames -x tailwindcss-classnames-expand.ts -o tailwindcss-classnames.ts"
  },
  "dependencies": {
    "antd": "^4.19.2",
    "less": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "less-loader": "^7.3.0",
    "postcss": "^8.4.12",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.0",
    "prettier-eslint": "^13.0.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.0.23",
    "tailwindcss-classnames": "^3.0.3",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-imp": "^2.1.6"
  }
}
